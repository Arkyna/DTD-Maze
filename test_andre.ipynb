    "import cv2\n",
    "import mediapipe as mp\n",
    "import time\n",
    "import numpy as np\n",
    "from PIL import Image, ImageSequence\n",
    "import math # Untuk menghitung jarak\n",
    "\n",
    "# --- PENGATURAN ---\n",
    "CHARACTER_IMAGE_PATH = './assets/denis-gerak-noBG.gif'\n",
    "IDLE_IMAGE_PATH = './assets/denis_diam.png'\n",
    "MAZE_IMAGE_PATH = './assets/maze1.png'\n",
    "\n",
    "CHARACTER_SIZE = (40, 40)  # <<< Kecilkan karakter agar pas di labirin\n",
    "MOVE_SPEED = 4\n",
    "DEAD_ZONE_SIZE = 40 # Sedikit diperbesar untuk stabilitas\n",
    "GAME_BG_COLOR = (255, 255, 255) # Latar Belakang Putih\n",
    "ANIMATION_DELAY = 0.1\n",
    "WALL_COLOR_THRESHOLD = 60 # Naikkan sedikit\n",
    "\n",
    "# --- <<< PENGATURAN BARU UNTUK LABIRIN >>> ---\n",
    "# Ukuran labirin yang akan ditampilkan di layar (lebar, tinggi)\n",
    "# Sesuaikan agar proporsional dengan gambar asli Anda & muat di window\n",
    "MAZE_DISPLAY_SIZE = (400, 300) # Contoh: labirin 400x300 piksel\n",
    "\n",
    "# Posisi sudut kiri atas labirin di game_window (x, y)\n",
    "# Kita akan letakkan di tengah window sebagai contoh\n",
    "# Ini akan dihitung setelah 'width' dan 'height' window diketahui\n",
    "\n",
    "# Titik spawn karakter (x,y) RELATIF terhadap SUDUT KIRI ATAS labirin yang sudah di-resize\n",
    "# Misalnya, jika ingin mulai sedikit di dalam dari kiri atas labirin:\n",
    "MAZE_SPAWN_POINT = (30, 30) # (x di labirin, y di labirin)\n",
    "\n",
    "# Titik goal (x,y) RELATIF terhadap SUDUT KIRI ATAS labirin yang sudah di-resize\n",
    "MAZE_GOAL_POINT = (370, 270) # Contoh: dekat sudut kanan bawah labirin\n",
    "GOAL_RADIUS = 20 # Seberapa dekat karakter harus ke goal\n",
    "# ---------------------------------------------\n",
    "\n",
    "# Fungsi overlay_image_alpha (Sama)\n",
    "def overlay_image_alpha(background, overlay, x, y, size):\n",
    "    if overlay is None: return\n",
    "    try: overlay_resized = cv2.resize(overlay, size)\n",
    "    except cv2.error as e: print(f\"Error resizing overlay: {e}\"); return\n",
    "    img_h, img_w, img_c = overlay_resized.shape; bg_h, bg_w, _ = background.shape\n",
    "    x_tl = x - img_w // 2; y_tl = y - img_h // 2\n",
    "    x1, x2 = max(0, x_tl), min(bg_w, x_tl + img_w); y1, y2 = max(0, y_tl), min(bg_h, y_tl + img_h)\n",
    "    ix1, ix2 = max(0, -x_tl), min(img_w, bg_w - x_tl); iy1, iy2 = max(0, -y_tl), min(img_h, bg_h - y_tl)\n",
    "    if x1 >= x2 or y1 >= y2 or ix1 >= ix2 or iy1 >= iy2: return\n",
    "    roi = background[y1:y2, x1:x2]; overlay_crop = overlay_resized[iy1:iy2, ix1:ix2]\n",
    "    if roi.shape[0] != overlay_crop.shape[0] or roi.shape[1] != overlay_crop.shape[1]:\n",
    "        overlay_crop = cv2.resize(overlay_crop, (roi.shape[1], roi.shape[0]))\n",
    "    if img_c == 4:\n",
    "        alpha_s = overlay_crop[:, :, 3] / 255.0; alpha_l = 1.0 - alpha_s\n",
    "        for c in range(0, 3): roi[:, :, c] = (alpha_s * overlay_crop[:, :, c] + alpha_l * roi[:, :, c])\n",
    "    else: background[y1:y2, x1:x2] = overlay_crop[:,:,:3]\n",
    "\n",
    "# Fungsi load_gif_frames (Sama)\n",
    "def load_gif_frames(gif_path):\n",
    "    frames_cv2 = [];\n",
    "    try:\n",
    "        with Image.open(gif_path) as img:\n",
    "            print(f\"Memuat GIF: {gif_path}...\");\n",
    "            for frame_pil in ImageSequence.Iterator(img):\n",
    "                frame_pil = frame_pil.convert('RGBA'); frame_np = np.array(frame_pil)\n",
    "                frame_bgra = cv2.cvtColor(frame_np, cv2.COLOR_RGBA2BGRA)\n",
    "                frames_cv2.append(frame_bgra)\n",
    "            print(f\"Berhasil memuat {len(frames_cv2)} frame GIF.\")\n",
    "    except Exception as e: print(f\"Error saat memuat GIF {gif_path}: {e}\")\n",
    "    return frames_cv2\n",
    "\n",
    "# Fungsi load_image_with_alpha (Sama)\n",
    "def load_image_with_alpha(image_path):\n",
    "    img = cv2.imread(image_path, cv2.IMREAD_UNCHANGED)\n",
    "    if img is None: print(f\"Error: Gagal memuat {image_path}\"); return None\n",
    "    h, w, *c = img.shape\n",
    "    if not c: img = cv2.cvtColor(img, cv2.COLOR_GRAY2BGRA)\n",
    "    elif c[0] == 3: img = cv2.cvtColor(img, cv2.COLOR_BGR2BGRA)\n",
    "    print(f\"Berhasil memuat gambar: {image_path}\"); return img\n",
    "\n",
    "# --- Fungsi Deteksi Dinding (Disederhanakan, maze_img adalah maze_final) ---\n",
    "def is_wall(x_local, y_local, maze_img_final, threshold=WALL_COLOR_THRESHOLD):\n",
    "    \"\"\"Memeriksa apakah piksel di (x_local, y_local) DALAM maze_img_final adalah dinding.\"\"\"\n",
    "    h_maze, w_maze, _ = maze_img_final.shape\n",
    "    if 0 <= x_local < w_maze and 0 <= y_local < h_maze:\n",
    "        b, g, r = maze_img_final[y_local, x_local][:3]\n",
    "        if maze_img_final.shape[2] == 4 and maze_img_final[y_local, x_local][3] < 128: # Cek transparansi\n",
    "            return False # Area transparan di labirin BUKAN dinding\n",
    "        return (int(b) + int(g) + int(r)) < threshold * 3\n",
    "    return True # Di luar batas labirin dianggap dinding\n",
    "\n",
    "# --- Fungsi Cek Kolisi Karakter (DIUBAH untuk koordinat relatif) ---\n",
    "def check_collision(char_center_x_game, char_center_y_game, char_size,\n",
    "                    maze_img_final, maze_pos_on_screen):\n",
    "    \"\"\"Memeriksa Bounding Box karakter (koordinat game) terhadap maze_img_final.\"\"\"\n",
    "    w_char, h_char = char_size\n",
    "    half_w_char, half_h_char = w_char // 2, h_char // 2\n",
    "    offset = 3 # Perkecil offset agar lebih pas\n",
    "\n",
    "    points_to_check_game_coords = [\n",
    "        (char_center_x_game - half_w_char + offset, char_center_y_game - half_h_char + offset),\n",
    "        (char_center_x_game + half_w_char - offset, char_center_y_game - half_h_char + offset),\n",
    "        (char_center_x_game - half_w_char + offset, char_center_y_game + half_h_char - offset),\n",
    "        (char_center_x_game + half_w_char - offset, char_center_y_game + half_h_char - offset),\n",
    "    ]\n",
    "    # Bisa tambahkan lebih banyak titik jika perlu (tengah sisi, dll)\n",
    "\n",
    "    for px_game, py_game in points_to_check_game_coords:\n",
    "        # Ubah koordinat titik pengecekan menjadi relatif terhadap labirin\n",
    "        px_local_maze = int(px_game - maze_pos_on_screen[0])\n",
    "        py_local_maze = int(py_game - maze_pos_on_screen[1])\n",
    "\n",
    "        if is_wall(px_local_maze, py_local_maze, maze_img_final):\n",
    "            # print(f\"Collision at local maze ({px_local_maze}, {py_local_maze}) from game ({px_game}, {py_game})\")\n",
    "            return True\n",
    "    return False\n",
    "# ------------------------------------------------------------------\n",
    "\n",
    "# --- Muat Semua Gambar ---\n",
    "character_frames = load_gif_frames(CHARACTER_IMAGE_PATH)\n",
    "idle_frame = load_image_with_alpha(IDLE_IMAGE_PATH)\n",
    "maze_image_original = load_image_with_alpha(MAZE_IMAGE_PATH)\n",
    "\n",
    "fallback_img = np.zeros((100, 100, 4), dtype=np.uint8); cv2.circle(fallback_img, (50, 50), 40, (0, 255, 0, 255), -1)\n",
    "if not character_frames: character_frames.append(fallback_img)\n",
    "if idle_frame is None: idle_frame = character_frames[0]\n",
    "character_frames_flipped = [cv2.flip(frame, 1) for frame in character_frames]\n",
    "idle_frame_flipped = cv2.flip(idle_frame, 1)\n",
    "\n",
    "# Inisialisasi Webcam & dapatkan ukuran window\n",
    "cap = cv2.VideoCapture(0)\n",
    "success, temp_frame = cap.read()\n",
    "if not success: print(\"Tidak bisa mengakses webcam.\"); exit()\n",
    "height, width, _ = temp_frame.shape # Ukuran window game = ukuran webcam\n",
    "temp_frame = None\n",
    "\n",
    "# --- Hitung Posisi Labirin di Tengah Window ---\n",
    "MAZE_POSITION_ON_SCREEN = (\n",
    "    (width - MAZE_DISPLAY_SIZE[0]) // 2,\n",
    "    (height - MAZE_DISPLAY_SIZE[1]) // 2\n",
    ")\n",
    "# ----------------------------------------------\n",
    "\n",
    "# --- Proses Gambar Labirin ---\n",
    "if maze_image_original is None:\n",
    "    print(f\"Error: Labirin tidak dimuat. Membuat labirin kosong.\")\n",
    "    maze_final = np.full((*MAZE_DISPLAY_SIZE[::-1], 3) if MAZE_DISPLAY_SIZE[0] > 0 and MAZE_DISPLAY_SIZE[1] > 0 else (100,100,3), (200, 200, 200), dtype=np.uint8) # Labirin abu-abu jika gagal\n",
    "    can_play_maze = False\n",
    "else:\n",
    "    maze_final = cv2.resize(maze_image_original, MAZE_DISPLAY_SIZE)\n",
    "    # Pastikan maze_final punya 3 channel untuk is_wall jika aslinya 4 channel dari load_image_with_alpha\n",
    "    if maze_final.shape[2] == 4:\n",
    "         print(\"Labirin punya alpha, mengambil BGR untuk collision.\")\n",
    "         # maze_final_collision = maze_final[:,:,:3] # Ini bisa jadi masalah jika alpha penting untuk deteksi jalan\n",
    "    else:\n",
    "         # maze_final_collision = maze_final\n",
    "         pass # sudah BGR\n",
    "    print(\"Labirin berhasil dimuat dan di-resize.\")\n",
    "    can_play_maze = True\n",
    "# ---------------------------------------------\n",
    "\n",
    "# --- Hitung Posisi Awal Karakter di Window Game ---\n",
    "obj_x = MAZE_POSITION_ON_SCREEN[0] + MAZE_SPAWN_POINT[0]\n",
    "obj_y = MAZE_POSITION_ON_SCREEN[1] + MAZE_SPAWN_POINT[1]\n",
    "# -------------------------------------------------\n",
    "\n",
    "initial_nose_x = None; initial_nose_y = None; center_set = False\n",
    "current_frame_index = 0; last_frame_time = time.time(); num_frames = len(character_frames)\n",
    "last_direction = \"KIRI\"\n",
    "game_won = False # <<< BARU: Status kemenangan\n",
    "win_message_display_time = 0\n",
    "\n",
    "print(\"Arahkan wajah ke tengah dan tunggu hingga garis silang muncul...\")\n",
    "\n",
    "while cap.isOpened():\n",
    "    success, image = cap.read()\n",
    "    if not success: print(\"Gagal membaca frame.\"); continue\n",
    "\n",
    "    # --- Buat Latar Belakang Putih ---\n",
    "    game_window = np.full((height, width, 3), GAME_BG_COLOR, dtype=np.uint8)\n",
    "    # ----------------------------------\n",
    "\n",
    "    # --- Overlay Labirin di atas BG Putih ---\n",
    "    if can_play_maze:\n",
    "        # Posisi untuk overlay_image_alpha adalah TENGAH gambar labirin di game_window\n",
    "        maze_draw_x = MAZE_POSITION_ON_SCREEN[0] + MAZE_DISPLAY_SIZE[0] // 2\n",
    "        maze_draw_y = MAZE_POSITION_ON_SCREEN[1] + MAZE_DISPLAY_SIZE[1] // 2\n",
    "        overlay_image_alpha(game_window, maze_final, maze_draw_x, maze_draw_y, MAZE_DISPLAY_SIZE)\n",
    "    # ------------------------------------------\n",
    "\n",
    "    image = cv2.flip(image, 1)\n",
    "    image_rgb = cv2.cvtColor(image, cv2.COLOR_BGR2RGB)\n",
    "    results = face_mesh.process(image_rgb)\n",
    "    move_x = 0; move_y = 0; status_text = \"CARI WAJAH...\"\n",
    "\n",
    "    if not game_won and results.multi_face_landmarks: # Hanya proses gerakan jika game belum dimenangkan\n",
    "        for face_landmarks in results.multi_face_landmarks:\n",
    "            nose_tip = face_landmarks.landmark[1]\n",
    "            current_nose_x = int(nose_tip.x * width); current_nose_y = int(nose_tip.y * height)\n",
    "            if not center_set: initial_nose_x = current_nose_x; initial_nose_y = current_nose_y; center_set = True; print(f\"Pusat: ({initial_nose_x}, {initial_nose_y})\")\n",
    "            cv2.circle(image, (current_nose_x, current_nose_y), 5, (0, 0, 255), -1)\n",
    "            if center_set:\n",
    "                dead_zone_x1 = initial_nose_x - DEAD_ZONE_SIZE // 2; dead_zone_x2 = initial_nose_x + DEAD_ZONE_SIZE // 2\n",
    "                dead_zone_y1 = initial_nose_y - DEAD_ZONE_SIZE // 2; dead_zone_y2 = initial_nose_y + DEAD_ZONE_SIZE // 2\n",
    "                if current_nose_y > dead_zone_y2: move_y = MOVE_SPEED; move_x = 0; status_text = \"BAWAH\"\n",
    "                elif current_nose_y < dead_zone_y1: move_y = -MOVE_SPEED; move_x = 0; status_text = \"ATAS\"\n",
    "                else:\n",
    "                    if current_nose_x > dead_zone_x2: move_x = MOVE_SPEED; move_y = 0; status_text = \"KANAN\"; last_direction = \"KANAN\"\n",
    "                    elif current_nose_x < dead_zone_x1: move_x = -MOVE_SPEED; move_y = 0; status_text = \"KIRI\"; last_direction = \"KIRI\"\n",
    "                    else: move_x = 0; move_y = 0; status_text = \"BERHENTI\"\n",
    "                cv2.line(image, (initial_nose_x, 0), (initial_nose_x, height), (0, 255, 255), 2); cv2.line(image, (0, initial_nose_y), (width, initial_nose_y), (0, 255, 255), 2)\n",
    "                cv2.rectangle(image, (dead_zone_x1, dead_zone_y1), (dead_zone_x2, dead_zone_y2), (255, 0, 0), 1)\n",
    "            else: status_text = \"TUNGGU PUSAT DIATUR...\"\n",
    "    elif game_won:\n",
    "        status_text = \"MENANG!\" # Jaga status agar animasi berhenti\n",
    "        move_x = 0\n",
    "        move_y = 0\n",
    "    else:\n",
    "        move_x = 0; move_y = 0; status_text = \"CARI WAJAH...\"\n",
    "\n",
    "    current_time = time.time()\n",
    "    if status_text != \"BERHENTI\" and status_text != \"MENANG!\" and num_frames > 1 and (current_time - last_frame_time > ANIMATION_DELAY):\n",
    "        current_frame_index = (current_frame_index + 1) % num_frames; last_frame_time = current_time\n",
    "\n",
    "    # --- Perbarui posisi dengan Cek Kolisi (Hanya jika game bisa dimainkan) ---\n",
    "    if can_play_maze and not game_won:\n",
    "        next_x = obj_x + move_x\n",
    "        next_y = obj_y + move_y\n",
    "        if not check_collision(next_x, obj_y, CHARACTER_SIZE, maze_final, MAZE_POSITION_ON_SCREEN):\n",
    "            obj_x = next_x\n",
    "        if not check_collision(obj_x, next_y, CHARACTER_SIZE, maze_final, MAZE_POSITION_ON_SCREEN):\n",
    "            obj_y = next_y\n",
    "    elif not game_won: # Jika tidak ada maze, gerak bebas (atau kondisi lain)\n",
    "        obj_x += move_x\n",
    "        obj_y += move_y\n",
    "    # ---------------------------------------------------------------------\n",
    "\n",
    "    char_w, char_h = CHARACTER_SIZE\n",
    "    obj_x = max(MAZE_POSITION_ON_SCREEN[0] + char_w // 2, min(MAZE_POSITION_ON_SCREEN[0] + MAZE_DISPLAY_SIZE[0] - char_w // 2, obj_x))\n",
    "    obj_y = max(MAZE_POSITION_ON_SCREEN[1] + char_h // 2, min(MAZE_POSITION_ON_SCREEN[1] + MAZE_DISPLAY_SIZE[1] - char_h // 2, obj_y))\n",
    "    # Batasi juga dengan window utama\n",
    "    obj_x = max(char_w // 2, min(width - char_w // 2, obj_x))\n",
    "    obj_y = max(char_h // 2, min(height - char_h // 2, obj_y))\n",
    "\n",
    "\n",
    "    # --- <<< Cek Kondisi Menang >>> ---\n",
    "    if can_play_maze and not game_won:\n",
    "        char_maze_x = obj_x - MAZE_POSITION_ON_SCREEN[0]\n",
    "        char_maze_y = obj_y - MAZE_POSITION_ON_SCREEN[1]\n",
    "        dist_to_goal = math.sqrt((char_maze_x - MAZE_GOAL_POINT[0])**2 + (char_maze_y - MAZE_GOAL_POINT[1])**2)\n",
    "        if dist_to_goal < GOAL_RADIUS:\n",
    "            game_won = True\n",
    "            win_message_display_time = time.time() # Catat waktu menang\n",
    "            print(\"SELAMAT, ANDA MENANG!\")\n",
    "    # ----------------------------------\n",
    "\n",
    "    # Pilih Frame\n",
    "    if status_text == \"BERHENTI\" or game_won: # Jika menang, tampilkan frame diam juga\n",
    "        frame_to_draw = idle_frame_flipped if last_direction == \"KANAN\" else idle_frame\n",
    "    elif status_text == \"KANAN\": frame_to_draw = character_frames_flipped[current_frame_index]\n",
    "    elif status_text == \"KIRI\": frame_to_draw = character_frames[current_frame_index]\n",
    "    else: frame_to_draw = character_frames_flipped[current_frame_index] if last_direction == \"KANAN\" else character_frames[current_frame_index]\n",
    "\n",
    "    overlay_image_alpha(game_window, frame_to_draw, obj_x, obj_y, CHARACTER_SIZE)\n",
    "\n",
    "    # --- Tampilkan Pesan Kemenangan ---\n",
    "    if game_won:\n",
    "        # Tampilkan pesan selama beberapa detik, lalu mungkin bisa keluar otomatis atau reset\n",
    "        win_text = \"ANDA MENANG!\"\n",
    "        text_size = cv2.getTextSize(win_text, cv2.FONT_HERSHEY_SIMPLEX, 2, 3)[0]\n",
    "        text_x = (width - text_size[0]) // 2\n",
    "        text_y = (height + text_size[1]) // 2\n",
    "        cv2.putText(game_window, win_text, (text_x, text_y), cv2.FONT_HERSHEY_SIMPLEX, 2, (0, 255, 0), 3, cv2.LINE_AA)\n",
    "        if time.time() - win_message_display_time > 5: # Tampilkan selama 5 detik\n",
    "            print(\"Waktu menang habis. Menutup program.\")\n",
    "            break # Keluar dari loop setelah menang\n",
    "    # -----------------------------------\n",
    "\n",
    "    cv2.putText(image, f\"Status: {status_text}\", (10, 30), cv2.FONT_HERSHEY_SIMPLEX, 1, (0, 255, 0), 2)\n",
    "    cv2.imshow('Controller', image); cv2.imshow('Game', game_window)\n",
    "    if cv2.waitKey(5) & 0xFF == ord('q'): break\n",
    "\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()"
   ]
  }
